<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite guice-stage="DEVELOPMENT" name="Default suite" parallel="tests"  thread-count="3">
	<parameter name="url" value="https://www.facebook.com/" />

	<test verbose="11" name="firefox">	
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="A1_1_WebDriver.SendParameterTo_WebDriver" />	
		</classes>
	</test>
	
	<test verbose="2" name="chrome">	
		<parameter name="browser" value="Chrome" />
		<classes>
			<class name="A1_1_WebDriver.SendParameterTo_WebDriver" />	
		</classes>
	</test>
	
</suite> 